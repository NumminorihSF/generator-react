/**
 * Type declaration for entity's data transfer object from API.
 *
 * @memberof <%= moduleName %>/api
 */
export type <%= Name %>ReadDto = any;

/**
* Type declaration for entity's data transfer object for API.
*
* @memberof <%= moduleName %>/api
*/
export type <%= Name %>WriteDto = any;

/**
 * Type declaration for entity's loading error data transfer object.
 *
 * @memberof <%= moduleName %>/api
 */
export type <%= Name %>LoadErrorDto = any;

/**
 * Type declaration for entity's saving error data transfer object.
 *
 * @memberof <%= moduleName %>/api
 */
export type <%= Name %>SaveErrorDto = any;

/**
 * Type declaration for entity's deleting data transfer object.
 *
 * @memberof <%= moduleName %>/api
 */
export type <%= Name %>DeleteDto = any;

/**
 * Type declaration for entity's deleting error data transfer object.
 *
 * @memberof <%= moduleName %>/api
 */
export type <%= Name %>DeleteErrorDto = any;

/**
 * Type declaration for entity's list in case of loading multiple instances.
 *
 * @memberof <%= moduleName %>/api
 */
export type <%= Name %>ListReadDto = {
  results: Array<<%= Name %>ReadDto>,
  // you may use pagination type if you have some.
  pagination: any,
};
<% } %>
