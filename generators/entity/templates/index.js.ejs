<% } %>/**
 * Module that implements work with <%= Name %> entities.
 *
 * @module <%= moduleName %>
 */
export * from './<%= Name %>.actions';
export * from './<%= Name %>.sagas';
export * from './<%= Name %>.schema';
export * from './<%= Name %>.selectors';
export * from './<%= Name %>.actionTypes';

/**
 * Injects entity into parent part of runtime.
 *
 * @private
 * @memberof <%= moduleName %>
 */
export function inject() {
  console.warn("Inject should be called via `import 'entities/<%= Name %>/inject';`");
  console.warn("Please don't use inject() from index.js");
  import('./<%= Name %>.inject');
}
