export function fetch<%= Name %>({ <%= name %>Id }<% if (flow) { %>: { <%= name %>Id: string }<% } %>) {
  return http.request({
    method: 'get',
    url: `/api/v1/<%= name %>/${<%= name %>Id}`,
  });
}

export function fetch<%= Name %>List({ query }<% if (flow) { %>: *<% } %>) {
  return http.request({
    method: 'get',
    url: `/api/v1/<%= name %>`,
    params: query,
  });
}

export function save<%= Name %>(<%= name %><% if (flow) { %>: <%= Name%>Dto<% } %>) {
