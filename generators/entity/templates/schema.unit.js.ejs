import Ajv from 'ajv';

import { <%= Name %>DtoSchema } from './<%= Name %>.schema';

const ajv = new Ajv();

describe('<%= [FeatureName.split('/').join('::'), 'Entity'].filter(Boolean).join('::') %>::<%= Name %>::schema', function() {
  describe('<%= Name %>DtoSchema', function() {
    it('uses valid JSON-schema', function() {
      const isValid = ajv.validateSchema(<%= Name %>DtoSchema);

      expect(isValid).to.be.equal(true);
    });
  });
});
