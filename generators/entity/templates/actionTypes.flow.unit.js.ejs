// @flow
// If check inside this file fails, it means that some actiontype has type 'string' instead of
// literal type.
import * as ActionType from './<%= Name %>.actionTypes';

for (const actionType of ActionType) {
  type <%= Name %>Action = typeof actionType;

  type <%= Name %>ActionMap<T> =
    & (<%= Name %>Action => <%= Name %>Action)
    & (string => empty);

  type Check<T> = $Call<<%= Name %>ActionMap<T>, T>;

  (actionType: Check<<%= Name %>Action>);
}
