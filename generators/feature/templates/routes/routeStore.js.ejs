<% if (flow) { %>// @flow
<% } %>import * as React from 'react';
import { RouteStore } from 'react-app-core';

import { ExampleHeader } from '../components/ExampleHeader';
import { ExampleRootPage } from '../pages/ExampleRootPage';
import { ExampleNestedPage } from '../pages/ExampleNestedPage';

export const <%= name %>RouteStore = new RouteStore()
  .setName('<%= name %>')
  .inject(
    [
      {
        path: '',
        component: ExampleHeader,
      },
      {
        path: '',
        exact: true,
        component: ExampleRootPage,
      },
      {
        path: 'nested',
        exact: true,
        component: ExampleNestedPage,
      },
    ],
  );

export const routeStore = <%= name %>RouteStore;
