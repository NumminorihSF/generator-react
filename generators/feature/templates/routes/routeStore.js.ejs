<% if (flow) { %>// @flow
<% } %>import * as React from 'react';
import { RouteStore } from 'react-app-core';

import { ExampleHeader } from '../components/ExampleHeader';
import { ExampleRootPage } from '../pages/ExampleRootPage';
import { ExampleNestedPage } from '../pages/ExampleNestedPage';

/**
 * RouteStore for <%= Name %> feature.
 *
 * Every route of this feature should be described in this section except nested features.
 * Currently available routes:
 * - featureRoot
 * - featureRoot/nested
 *
 * @type {ReactAppCode.RouteStore} See react-app-core to learn more about it.
 * @memberof <%= moduleName %>/Routing/routes
 */
export const <%= name %>RouteStore = new RouteStore()
  .setName('<%= name %>')
  .inject(
    [
      {
        path: '',
        component: ExampleHeader,
      },
      {
        path: '',
        exact: true,
        component: ExampleRootPage,
      },
      {
        path: 'nested',
        exact: true,
        component: ExampleNestedPage,
      },
    ],
  );

/**
 * Used for RouteStores' composition.
 *
 * @memberof <%= moduleName %>/Routing/routes
 */
export const routeStore = <%= name %>RouteStore;
